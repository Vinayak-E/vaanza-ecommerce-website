 <script src="https://cdn.jsdelivr.net/npm/notyf@3/notyf.min.js"></script>

 <script>
    document.addEventListener('DOMContentLoaded', () => {
      console.log('DOM fully loaded and parsed');
      
      const addToCartButton = document.getElementById('add-to-cart');
      console.log('Add to Cart Button:', addToCartButton);
     
      if (addToCartButton) {
        addToCartButton.addEventListener('click', async (event) => {
          event.preventDefault();
     
          const productId = document.getElementById('productId').value.trim();
          const variantId = document.getElementById('variantId').value.trim();
          const size = document.getElementById('size').value.trim();
          const quantity = document.getElementById('qty').value.trim();
     
          console.log(`Product ID: ${productId}`);
          console.log(`Variant ID: ${variantId}`);
          console.log(`Size: ${size}`);
          console.log(`Quantity: ${quantity}`);
     
          if (!size) {
            document.getElementById('sizeErrP').style.display = 'block';
            return;
          } else {
            document.getElementById('sizeErrP').style.display = 'none';
          }
     
          const data = {
            productId,
            variantId,
            size,
            quantity
          };
     
          console.log('Data to be sent:', data);
     
          try {
            const response = await fetch('/add-to-cart', {
              method: 'POST',
              headers: {
                'Content-Type': 'application/json'
              },
              body: JSON.stringify(data)
            });
     
            if (response.ok) {
    Swal.fire({
        icon: 'success',
        title: 'Success',
        text: 'Product added to cart successfully!',
        position: 'bottom',
        showConfirmButton: false,
        timer: 1500,
        customClass: {
            popup: 'custom-swal-popup',
            title: 'custom-swal-title',
            content: 'custom-swal-content'
        },
        background: '#f0f9ff',
        iconColor: '#28a745'
    }).then(() => {
        window.location.href = '/cart';
    });
} else {
    Swal.fire({
        icon: 'error',
        title: 'Error',
        text: 'Failed to add product to cart.',
        position: 'bottom',
        showConfirmButton: false,
        timer: 1500,
        customClass: {
            popup: 'custom-swal-popup',
            title: 'custom-swal-title',
            content: 'custom-swal-content'
        },
        background: '#ffebeb',
        iconColor: '#dc3545'
    });
}
} catch (error) {
    console.error('Error:', error);
    Swal.fire({
        icon: 'error',
        title: 'Error',
        text: 'An error occurred while adding product to cart.',
        position: 'bottom',
        showConfirmButton: false,
        timer: 1500,
        customClass: {
            popup: 'custom-swal-popup',
            title: 'custom-swal-title',
